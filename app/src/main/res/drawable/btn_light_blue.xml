<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true"   android:state_focused="false"  >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1dp" android:color="#FA0202" />
            <solid android:color="@color/fet_higlight_blue"></solid>
        </shape>
    </item>

    <item android:state_pressed="true"   >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1dp" android:color="#FFEB3B" />
            <solid android:color="@color/fet_higlight_blue"></solid>
        </shape>
    </item>

    <item android:state_focused="true"  android:state_selected="false"  >
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1dp" android:color="#8BC34A" />
            <solid android:color="@color/fet_higlight_blue"></solid>
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <stroke android:width="1dp" android:color="#FAF7F7" />
            <solid android:color="@color/fet_higlight_white"></solid>
        </shape>
    </item>

</selector>